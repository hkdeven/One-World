<h1>Welcome to One World</h1>

<%= form_for(:session, url: login_path) do |f| %>
<p>
<%= f.label :email %>
<%= f.text_field :email %>
</p>
<p>
<%= f.label :password %>
<%= f.password_field :password %>
</p>
<p>
  <%= f.submit "Log in", class: "btn-submit" %> |
  <%= link_to "Sign Up", 'signup', class: "btn-submit" %>
</p>
<% end %>
